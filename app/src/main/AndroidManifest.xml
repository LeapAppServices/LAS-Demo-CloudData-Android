<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.maxleap.demo.clouddata">

    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.ACCESS_NETWORK_STATE" />
    <uses-permission android:name="android.permission.READ_PHONE_STATE" />
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.ACCESS_FINE_LOCATION" />
    <uses-permission android:name="android.permission.ACCESS_COARSE_LOCATION" />

    <application
        android:name="com.maxleap.demo.clouddata.App"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme">

        <meta-data
            android:name="ml_channel"
            android:value="google_play" />

        <activity
            android:name="com.maxleap.demo.clouddata.MainActivity"
            android:label="@string/app_name">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <activity android:name="com.maxleap.demo.clouddata.activities.CoreActivity" />
        <activity android:name="com.maxleap.demo.clouddata.activities.DataTypeActivity" />
        <activity android:name="com.maxleap.demo.clouddata.activities.FileActivity" />
        <activity android:name="com.maxleap.demo.clouddata.activities.GeoActivity" />
        <activity android:name="com.maxleap.demo.clouddata.activities.OperationActivity" />
        <activity android:name="com.maxleap.demo.clouddata.activities.QueryActivity" />
        <activity android:name="com.maxleap.demo.clouddata.activities.SubClassActivity" />
        <activity android:name="com.maxleap.demo.clouddata.activities.UserActivity" />
    </application>

</manifest>
